    "import re\n",
    "\n",
    "def find_phone_numbers(text):\n",
    "    pattern = re.compile(r'(7-\\d{3}-\\d{3}-\\d{2}-\\d{2}|8-\\d{3}-\\d{4}-\\d{4})')\n",
    "\n",
    "    matches = re.findall(pattern, text)\n",
    "\n",
    "    for match in matches:\n",
    "        print(match)\n",
    "\n",
    "find_phone_numbers(input())"
   ]
